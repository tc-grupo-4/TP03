\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
f_{0}=\frac{\sqrt{2+\frac{R_{2}}{R_{1}}}}{20.\pi.C_{2}R_{2}}\label{eq:4}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
donde 
