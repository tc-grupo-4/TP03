\begin_layout Standard

\lang english
\begin_inset Formula 
\[
H(j.\omega_{0})=A_{0}=\frac{\left(\frac{j\omega_{0}}{\omega_{0}}\right)^{2}+\frac{j\omega_{0}}{\omega_{0}.Q_{c}}+1}{\left(\frac{j\omega_{0}}{\omega_{0}}\right)^{2}+\frac{j\omega_{0}}{\omega_{0}.Q_{p}}+1}
