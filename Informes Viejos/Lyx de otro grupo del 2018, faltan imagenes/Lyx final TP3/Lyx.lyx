\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
Q_{c}=\frac{\left(2R_{1}+R_{2}\right).10R_{2}}{\sqrt{2+\frac{R_{2}}{R_{1}}}\left(-9.x.R_{2}^{2}-x^{2}.R_{2}^{2}+R_{1}^{2}+31.R_{1}.R_{2}+10.R_{2}^{2}\right)}\label{eq:5}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
Q_{p}=\frac{\left(2R_{1}+R_{2}\right).10R_{2}}{\sqrt{2+\frac{R_{2}}{R_{1}}}\left(-x^{2}.R_{2}^{2}+11.x.R_{2}^{2}+.R_{1}^{2}+31.R_{1}.R_{2}\right)}\label{eq:6}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
Luego se hallo A
\begin_inset Formula $_{0}$
