status open

\begin_layout Plain Layout
\align center
\begin_inset Graphics
	filename Resources/RtaEnFrecuanciaParametrica.png
	lyxscale 60
	scale 60

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Respuesta en frecuencia paramétrica para una frecuencia dada
\end_layout

\end_inset


