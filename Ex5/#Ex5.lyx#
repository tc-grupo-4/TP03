\begin_body

\begin_layout Part
Control de tonos y ecualizador de fase
\end_layout

\begin_layout Standard
A lo largo de esta parte, se pondra foco en el circuito mostrado en la Figura
 
\begin_inset CommandInset ref
LatexCommand ref
reference "5_1"

\end_inset

, que se trata de un circuito de control de tonos.
\end_layout

\begin_layout Standard
\begin_inset Float figure
wide false
sideways false
status open

\begin_layout Plain Layout
\align center
\begin_inset Graphics
	filename Resources/Circuito.png
	lyxscale 50
	scale 50

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Circuito de Control de Tonos
\end_layout

\end_inset


\begin_inset CommandInset label
LatexCommand label
name "5_1"

\end_inset


\end_layout

\begin_layout Plain Layout

\end_layout

\end_inset


\end_layout

\begin_layout Section
Transferencia
\end_layout

\begin_layout Standard
Al calcular la transferencia genericamente para cualquier valor de impedancias,
 y llamando a 
\begin_inset Formula $R_{21}=R_{2}K\,\,,\,\,R_{22}=R_{2}(1-K)$
\end_inset

, el calculo de la transferencia se expresa como la ecuación (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:5_1"

\end_inset

).
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
H(s)=\frac{As^{2}+Bs+C}{As^{2}+Es+C}\label{eq:5_1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
A=20C_{2}^{2}K^{2}R_{1}R_{2}^{2}-20C_{2}^{2}KR_{1}R_{2}^{2}-10C_{2}^{2}R_{1}^{2}R_{2}-100C_{2}^{2}R_{1}R_{2}^{2}\approx-100C_{2}^{2}R_{1}R_{2}^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
B=C_{2}K^{2}R_{2}^{2}+9C_{2}KR_{2}^{2}-C_{2}R_{1}^{2}-31C_{2}R_{1}R_{2}-10C_{2}R_{2}^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C=-2R_{1}-R_{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E=C_{2}K^{2}R_{2}^{2}-11C_{2}KR_{2}^{2}-C_{2}R_{1}^{2}-31C_{2}R_{1}R_{2}
\]

\end_inset


\end_layout

\begin_layout Section
Análisis de frecuencia central
\end_layout

\begin_layout Standard
Para obtener la frecuencia central, basta con llevar a la ecuación de transferen
cia de la siguiente manera
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
H(s)=\frac{\left(\frac{s}{\omega_{0}}\right)^{2}+\frac{s}{Q_{z}\omega_{0}}+1}{\left(\frac{s}{\omega_{0}}\right)^{2}+\frac{s}{Q_{p}\omega_{0}}+1}\label{eq:5_2}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Por lo tanto;
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{1}{\omega_{0}^{2}}=\frac{A}{C}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\Rightarrow\omega_{0}=\frac{\sqrt{2+\frac{R_{2}}{R_{1}}}}{10C_{2}R_{2}}\Longrightarrow f_{0}=\frac{\sqrt{2+\frac{R_{2}}{R_{1}}}}{20\pi C_{2}R_{2}}\label{eq:5_6}
\end{equation}

\end_inset


\end_layout

\begin_layout Section
Análisis paramétrico
\end_layout

\begin_layout Standard
Si se analiza los factores de calidad correspondientes se obtiene:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Q_{z}=\frac{C}{B\omega_{0}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Q_{z}=-\frac{10C_{2}\sqrt{R_{1}}R_{2}\sqrt{2R_{1}+R_{2}}}{C_{2}K^{2}R_{2}^{2}+9C_{2}KR_{2}^{2}-C_{2}R_{1}^{2}-31C_{2}R_{1}R_{2}-10C_{2}R_{2}^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Q_{p}=\frac{C}{E\omega_{0}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Q_{p}=-\frac{10C_{2}\sqrt{R_{1}}R_{2}\sqrt{2R_{1}+R_{2}}}{-C_{2}K^{2}R_{2}^{2}+11C_{2}KR_{2}^{2}+C_{2}R_{1}^{2}+31C_{2}R_{1}R_{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
por lo tanto, la ganancia para la frecuencia central del filtro quedará
 dada por:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
A=\frac{R_{1}^{2}+31R_{1}R_{2}+10R_{2}^{2}}{R_{1}\left(R_{1}+31R_{2}\right)}\approx\frac{3R_{1}+R_{2}}{3R_{1}}\,K=0\label{eq:5_4}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
A=\frac{R_{1}\left(R_{1}+31R_{2}\right)}{R_{1}^{2}+31R_{1}R_{2}+10R_{2}^{2}}\approx\frac{3R_{1}}{R_{2}+3R_{1}}\,K=1\label{eq:5_5}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
A=\frac{-K^{2}R_{2}^{2}-9KR_{2}^{2}+R_{1}^{2}+31R_{1}R_{2}+10R_{2}^{2}}{-K^{2}R_{2}^{2}+11KR_{2}^{2}+R_{1}^{2}+31R_{1}R_{2}}\,\,\,Expresado\,Parametricamente\,en\,K
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Float figure
wide false
sideways false
status open

\begin_layout Plain Layout
\align center
\begin_inset Graphics
	filename Resources/DiagramaParametrico.png
	lyxscale 50
	scale 50

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Diagrama paramétrico
\end_layout

\end_inset


\end_layout

\begin_layout Plain Layout

\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Float figure
