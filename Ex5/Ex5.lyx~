\begin_layout Section
Transferencia
\end_layout

\begin_layout Standard
Al calcular la transferencia genericamente para cualquier valor de impedancias,
 y llamando a 
\begin_inset Formula $R_{2}=R_{2}^{'}+R_{2}^{''}$
\end_inset

, el calculo de la transferencia se expresa como la ecuaci√≥n (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:5_1"

\end_inset

).
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
H(s)=-G\,\frac{\frac{s^{2}}{\omega_{0}^{2}}+\left(\frac{2\xi_{1}}{\omega_{0}}\right)s+1}{\frac{s^{2}}{\omega_{0}^{2}}+\left(\frac{2\xi_{2}}{\omega_{0}}\right)s+1}\label{eq:5_1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
G=\frac{A_{vol}R_{1}+2R_{2}k}{A_{vol}R_{1}+2A_{vol}R_{2}k+2R_{1}+4R_{2}k}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{2\xi_{1}}{\omega_{0}}=\frac{2C_{1}R_{1}R_{2}k+C_{2}R_{1}R_{2}(1-k)+C_{2}R_{2}^{2}k(1-k)+C_{2}R_{2}kR_{3}}{R_{1}+2R_{2}k}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{1}{\omega_{0}^{2}}=\frac{C_{1}C_{2}R_{1}R_{2}^{2}k(1-k)}{R_{1}+2R_{2}k}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{2\xi_{2}}{\omega_{0}}=\frac{2A_{vol}C_{1}R_{1}R_{2}k+A_{vol}C_{2}R_{1}^{2}+A_{vol}C_{2}R_{1}R_{2}k+A_{vol}C_{2}R_{1}R_{2}(1-k)+A_{vol}C_{2}R_{1}R_{3}+A_{vol}C_{2}R_{2}^{2}k(1-k)+A_{vol}C_{2}R_{2}kR_{3}+4C_{1}R_{1}R_{2}k+C_{2}R_{1}^{2}+C_{2}R_{1}R_{2}k+2C_{2}R_{1}R_{2}(1-k)+C_{2}R_{1}R_{3}+2C_{2}R_{2}^{2}k(1-k)+2C_{2}R_{2}kR_{3}}{A_{vol}R_{1}+2A_{vol}R_{2}k+2R_{1}+4R_{2}k}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{1}{\omega_{0}^{2}}=\frac{2A_{vol}C_{1}C_{2}R_{1}^{2}R_{2}k+A_{vol}C_{1}C_{2}R_{1}R_{2}^{2}k(1-k)+2A_{vol}C_{1}C_{2}R_{1}R_{2}kR_{3}+2C_{1}C_{2}R_{1}^{2}R_{2}k+2C_{1}C_{2}R_{1}R_{2}^{2}k(1-k)+2C_{1}C_{2}R_{1}R_{2}kR_{3}}{A_{vol}R_{1}+2A_{vol}R_{2}k+2R_{1}+4R_{2}k}
\]

\end_inset


\end_layout

