\begin_layout Standard
Introduciendo esta condición, el sistema de ecuaciones que modela el circuito
 es el siguiente
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{cases}
V_{Out,1}=V_{A}=A\cdot\left(V_{1}^{+}-V_{1}^{-}\right) & \text{Ecuación OpAmp 1 (U1)}\\
V_{Out,2}=A\cdot\left(V_{2}^{+}-V_{2}^{-}\right) & \text{Ecuación OpAmp 2 (U2)}\\
\frac{V_{Out,1}-V_{1}^{-}}{R_{3}}+\frac{\widetilde{V}-V_{1}^{-}}{R_{4}}=0 & \text{Ecuación Nodo \ensuremath{V_{1}^{-}}}\\
\frac{V_{Out,3}-\widetilde{V}}{R_{5}}+\frac{V_{1}^{-}-\widetilde{V}}{R_{4}}+\frac{V_{2}^{-}-\widetilde{V}}{R_{6}}=0 & \text{Ecuación Nodo \ensuremath{\widetilde{V}}}\\
\frac{\widetilde{V}-V_{2}^{-}}{R_{6}}+\frac{V_{Out,2}-V_{2}^{-}}{R_{7}}=0 & \text{Ecuación Nodo \ensuremath{V_{2}^{-}}}\\
V_{Out,2}=V_{B}=0 & \text{Tierra Virtual}
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
Resolviendo el sistema se tiene que
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
V_{A,Real,U4Ideal}=\frac{A\cdot\left[\left(R_{3}\cdot R_{7}+R_{4}\cdot R_{7}\right)\cdot V_{1}-\left(R_{3}\cdot R_{6}+R_{3}\cdot R_{7}\right)\cdot V_{2}\right]}{R_{7}\cdot(R_{3}+R_{4}+A\cdot R_{4})}
\]

\end_inset


\end_layout

\begin_layout Standard
Con lo que, análogamente con lo analizado anteriormente
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
G_{C,A,Real,U4Ideal}=\frac{A\cdot\left[R_{4}\cdot R_{7}-R_{3}\cdot R_{6}\right]}{R_{7}\cdot(R_{3}+R_{4}+A\cdot R_{4})}\Rightarrow\Delta V_{AB,C,Real,U4Ideal}=G_{C,A,Real,U4Ideal}\cdot V_{C}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
G_{D,A,Real,U4Ideal}=\frac{A\cdot\left[2\cdot R_{3}\cdot R_{7}+R_{4}\cdot R_{7}+R_{3}\cdot R_{6}\right]}{2\cdot R_{7}\cdot(R_{3}+R_{4}+A\cdot R_{4})}\Rightarrow\Delta V_{AB,D,Real,U4Ideal}=G_{D,A,Real,U4Ideal}\cdot V_{D}
\]

\end_inset


\end_layout

\begin_layout Standard
Finalmente, considerando polo dominante
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
G_{C,A,Real,PD,U4Ideal}=\frac{A_{0}\cdot\left[R_{4}\cdot R_{7}-R_{3}\cdot R_{6}\right]}{R_{7}\cdot\left(\frac{s}{\mathrm{wp}}\left(R_{3}+R_{4}\right)+R_{3}+R_{4}\cdot\left(A_{0}+1\right)\right)}\label{eq:part2crealpdu4ideal}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Rightarrow\Delta V_{AB,C,Real,PD,U3Ideal}=G_{C,A,Real,PD,U4Ideal}\cdot V_{C}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
G_{D,A,Real,PD,U4Ideal}=\frac{A_{0}\cdot\left[2\cdot R_{3}\cdot R_{7}+R_{4}\cdot R_{7}+R_{3}\cdot R_{6}\right]}{2\cdot R_{7}\cdot\left(\frac{s}{\mathrm{wp}}\left(R_{3}+R_{4}\right)+R_{3}+R_{4}\cdot\left(A_{0}+1\right)\right)}\label{eq:part2drealpdu4ideal}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Rightarrow\Delta V_{AB,D,Real,PD,U3Ideal}=G_{D,A,Real,PD,U4Ideal}\cdot V_{C}
\]

\end_inset


